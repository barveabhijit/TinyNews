/* Body
--------------------------------------------------------------------------------------- */
.content{}
.ContentCategory{width:375px;margin-bottom:11px;margin-left:auto;margin-right:auto;margin-top:6px;}
.ContentCategory label{font-family: 'OpenSansCondensedBold'; font-size:16px;color:#333; margin-right:8px}
.ContentCategory select{width:280px!important;border:none;-webkit-border-radius: 5px;-moz-border-radius: 5px;
border-radius: 5px; background:url(../images/story/test.gif) no-repeat;height:27px; padding:3px 0 3px 5px;width:292px;-webkit-appearance: none;-webkit-box-shadow:  -.5px -.5px 10px 0px #999;box-shadow:  -.5px -.5px 10px 0px #999;}
article.story{margin:15px 0}
.item {width: 225px;margin: 10px;float: left}
.storyBlcok{width:201px;padding:12px; padding-bottom:0;background-color:#fff;-webkit-box-shadow:-.5px 1px 5px 2px #cacaca;box-shadow:  -.5px 1px 5px 2px #cacaca}
section header{padding:0; height:20px}
section h1{font-size:18px;text-transform:uppercase;}
section img{margin-bottom:10px;}
 
#imageContainer {margin: 0px auto;width:100%;}
 
.storyImage{cursor:pointer;width:200px;vertical-align:middle}
 
.storyHeadline{cursor:pointer; overflow:hidden; text-overflow:ellipsis;}
 
.storyInfo{cursor:pointer}
.storyInfo ul{}
.storyInfo ul li{float:left;text-align:left}
.storyInfo ul li a{font-size:12px;color:#13345c}
.storyInfo p{margin:5px 0;font-size:12px;color:#333;word-wrap:break-word}
 
.storyComment{width:225px;background-color:#f5f5f5;border-bottom:1px solid #ccc;padding:5px 0px;margin-top:15px; margin-left:-12px;}
.storyCommentList{}
.storyCommentList li{float:left;width:54px; height:30px}
.storyCommentList .like{ margin-left:3px}
.storyCommentList .dislike{ margin-right:4px}
.storyCommentList .likeNormal{background:url(../images/story/like-button-normal.png) no-repeat left center}
.storyCommentList .likeNormal:hover{background:url(../images/story/like-button-hover.png) no-repeat left center}
.storyCommentList .likeClicked{background:url(../images/story/like-button-active.png) no-repeat left center}
.storyCommentList .likeSelected{background:url(../images/story/like-button-selected.png) no-repeat left center}
.storyCommentList .dislikeNormal{background:url(../images/story/dislike-button-normal.png) no-repeat left center}
.storyCommentList .dislikeNormal:hover{background:url(../images/story/dislike-button-hover.png) no-repeat left center}
.storyCommentList .dislikeClicked{background:url(../images/story/dislike-button-active.png) no-repeat left center}
.storyCommentList .dislikeSelected{background:url(../images/story/dislike-button-selected.png) no-repeat left center}
.storyCommentList .comment{background:url(../images/story/comment-button-normal.png) no-repeat left center}
.storyCommentList .comment:hover{background:url(../images/story/comment-button-hover.png) no-repeat left center}
.storyCommentList .reclip{background:url(../images/story/repin-button-normal.png) no-repeat left center}
.storyCommentList .reclip:hover{background:url(../images/story/repin-button-hover.png) no-repeat left center}
.storyCommentList li.last{margin:0}
.storyCommentList li:hover{cursor:pointer}
 
.storyPerson{padding:10px 0}
.storyPerson img{float:left;margin-right:5px}
.storyPerson h2{font-size:11px;color:#000;font-weight:normal;text-transform:none;font-family: 'OpenSansCondensedLight';overflow:hidden;white-space:nowrap;text-overflow:ellipsis;-o-text-overflow:ellipsis}
.storyPerson h2 a{color:#13345c;margin-left:5px; font-size:12px;font-family: 'OpenSansCondensedBold';}
.storyPerson p{font-size:11px;color:#666;font-weight:normal;margin:5px 0}
.Personinfo{}
.Personinfo li{float:left;margin-right:13px; color: #666;
   font-family: OpenSansCondensedBold,Arial Narrow,san-serif;
   font-size: 11px;
   font-weight: normal;}
.Personinfo li.last{margin:0}
.Personinfo li a{font-size:11px;color:#666}
 
.storyComments{background-color:#f5f5f5;width:195px;margin-left:-12px;padding:10px 15px;border-top:1px solid #ccc}
.storyComments .allStoryBlocks{font-size:13px;font-weight:bold;color:#13345c;margin-left:50px;cursor:pointer;font-family: 'OpenSansCondensedBold';}
.storyComments .allStoryBlocks span{font-size:13px;font-weight:bold;color:#13345c;cursor:pointer}
.CommentBlock{overflow-x:hidden}
.CommentBlock img{float:left;margin:0 10px 0 0}
.CommentBlock h3{font-size:12px;color:#13345c}
.CommentBlock h3 a{font-size:12px}
.CommentBlock p{font-size:11px;color:#666;font-weight:normal}
form{margin:5px 0}
.CommentUser{float:left;margin-right:5px; margin-left:-6px}
.CommentText{width:154px;border:1px solid #ccc;padding:8px 5px;-webkit-border-radius: 5px;-moz-border-radius: 5px;border-radius: 5px;color:#999;font-size:14px;font-family: 'OpenSansLight';margin-right:-6px;margin-top:-1px}
 
.storyViaLink{margin-left: 0px !important}