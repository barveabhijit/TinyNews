.darkBlue-button {
width:auto;
background: #0b68a8; /* Old browsers */
/* IE9 SVG, needs conditional override of 'filter' to 'none' */
background: url(data:image/svg+xml;base64,);
background: -moz-linear-gradient(top, #0b68a8 0%, #0b68a8 34%, #13345c 97%, #13345c 100%); /* FF3.6+ */
background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#0b68a8), color-stop(34%,#0b68a8), color-stop(97%,#13345c), color-stop(100%,#13345c)); /* Chrome,Safari4+ */
background: -webkit-linear-gradient(top, #0b68a8 0%,#0b68a8 34%,#13345c 97%,#13345c 100%); /* Chrome10+,Safari5.1+ */
background: -o-linear-gradient(top, #0b68a8 0%,#0b68a8 34%,#13345c 97%,#13345c 100%); /* Opera 11.10+ */
background: -ms-linear-gradient(top, #0b68a8 0%,#0b68a8 34%,#13345c 97%,#13345c 100%); /* IE10+ */
background: linear-gradient(to bottom, #0b68a8 0%,#0b68a8 34%,#13345c 97%,#13345c 100%); /* W3C */
filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#0b68a8', endColorstr='#13345c',GradientType=0 ); /* IE6-8 */
border:1px solid #13345c;
color:#fff;
cursor:pointer;
display:inline-block;
font-family: 'OpenSansCondensedBold',Arial Narrow, sans-serif;
font-size:12pt;
font-weight:normal;
line-height:1;
outline:none;
position:relative;
text-align:center;
text-decoration:none;
text-shadow: 0 -1px #333;
box-shadow: 0 0 3px rgba(0, 0, 0, 0.7);
-webkit-box-shadow: 0 0 3px rgba(0, 0, 0, 0.7);
-moz-box-shadow: 0 0 3px rgba(0, 0, 0, 0.7);
margin:0;
padding:10px 12px 8px;
border-radius: 6px;
-webkit-border-radius: 6px;
-moz-border-radius: 6px;
}

