/* Header
--------------------------------------------------------------------------------------- */
header {
	height: 25px;
	padding: 10px 0;
	position: relative;
	background: #0b68a8
}
header h1 {
	float: left;
	text-transform: capitalize
}
header .logo {
	float: left;
	width: 15%;
	height: 24px
}
header nav {
	float: right;
	width: 30%
}
header nav ul {
	float: right;
	margin-top: 5px
}
header nav ul li {
	float: left;
	margin-right: 15px
}
header nav ul li.add {
	width: 45px
}

header .popup{display:none}
header nav ul li:hover .popup{display:block;position:absolute;top:30px;}
header nav ul li a:hover span{border-top: 4px solid #50B5E5;}


header nav ul li.notifications {
	margin: 0
}
header nav ul li.add a img {
	margin-top: 0
}
header nav ul li.user {
	width: 70px
}
header nav ul li.signout {
	width: 60px
}
header nav ul li.last {
	margin: 0
}
header nav ul li a {
	margin-right: 7px;
	font-size: 14px;
	color: #fff
}
header nav ul li a img {
	float: left;
	margin-top: -5px;
	margin-right: 10px
}
/* Main
--------------------------------------------------------------------------------------- */
.Content {
	margin-top: 50px
}
/* Sidebar
--------------------------------------------------------------------------------------- */
aside {
	width: 18%;
	float: left;
	margin-right: 60px
}
aside h1 {
	font-size: 24px
}
aside .sidebrList {
	border-bottom: 1px solid #ccc;
	height: 65px;
	padding: 20px 0 15px;
	position: relative
}
aside .sidebrList.last {
	border: none
}
aside .sidebrList .imageContainer {
	width: 50px;
	height: 50px;
	float: left;
	margin-right: 5px
}
aside .sidebrList .imageContainer img {
	width: 100%;
	height: 100%
}
aside .sidebrList h2 {
	font-family: 'OpenSansCondensedBold';
	font-size: 16px;
	text-transform: none
}
aside .sidebrList h2 a {
	font-size: 16px
}
aside .sidebrList h3 {
	font-family: 'OpenSansSemibold';
	font-size: 11px;
	position: absolute;
	bottom: 10px;
	right: 0;
	text-transform: none
}
aside .sidebrList h3 a {
	font-size: 11px
}
