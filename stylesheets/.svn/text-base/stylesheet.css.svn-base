/* User Profile Section
-----------------------*/
.userProfile{width:245px;padding:44px 25px 0 20px; background:#f7f7f7; font-family: 'OpenSansCondensedBold';min-height:1132px}
.userInfo{position:relative; margin-bottom:10px}
