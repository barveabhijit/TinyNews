/* Edit Content
---------------------------------------------------------------------- */

.content {
	background: #73c2e7 url(../images/general/bgrd.jpg) repeat;
	padding: 15px 0 50px;
}
.edit {
	width: 716px;
	margin: 0 auto;
	border: 1px solid #ccc;
	background-color: #fff;
	padding: 0 50px
}
.edit h1 {
	text-transform: uppercase;
	color: #06315d;
	font-family: 'OpenSansExtrabold';
}
.hRuler {
	background: url(../images/general/hRuler-ng.png) -70px 0px no-repeat;
	width: 853px;
	height: 1px;
	display: block;   
}
label {
	font-size: 18px;
	color: #333
}
label.secrecy {
	font-size: 14px
}
input[type="text"], input[type="email"] {
	width: 192px;
	height: 25px;
	line-height: 1.6em;
-webkit-border-radius: 5px-moz-border-radius: 5px;
	border-radius: 5px;
	border: 1px solid #ccc;
	padding: 0 10px;
	/*margin: 0 5px 0 50px*/
    margin: 0 5px 0 0px
}
.formBlock {
	width: 610px;
	margin: 15px auto;
	font-family: 'OpenSansCondensedLight';
	position: relative
}
.formName {
	width: 80px;
	text-align: right;
	float: left;
}
.form_btn {
	background: url(../images/general/btnChangePassword-bg.png) repeat-x;
	height: 33px;
	text-align: center;
	border: 1px solid #2484be;
	color: #fff;
	-webkit-border-radius: 5px;
	-moz-border-radius: 5px;
	border-radius: 5px;
	margin-left: 50px;
	font-weight: bold
}
.profileImage {
	float: left;
	margin-bottom: 15px;
	cursor: pointer;
	margin-left: 50px
}
.btnOn {
	float: left;
	display: block;
	margin-left: 50px;
	margin-right: 20px;
	cursor: pointer;
}
.btnOff {
	float: left;
	display: block;
	margin-left: 130px;
	margin-right: 20px;
	cursor: pointer;
}
.btnLearnmore {
	position: absolute;
	right: 0;
	top: 60px;
	color: #06315d;
	font-size: 14px;
	font-weight: bold
}
.form_btn_save {
	background: url(../images/general/btnSave-bg.png) repeat-x;
	height: 49px;
	border: 1px solid #ccc;
	text-align: center;
	color: #ccc;
	font-size: 24px;
	font-weight: bold;
	-webkit-border-radius: 5px;
	-moz-border-radius: 5px;
	border-radius: 5px;
	margin-left: 130px;
	margin-bottom: 30px;
	padding: 0 20px
}

