

/* Login & Registration
--------------------------------------------------------------------------------------- */
.login{margin:40px 0}
.login h2{font-family: 'OpenSansCondensedBold';font-size:72px;color:#808080; float:left; margin:50px 60px}
.loginFacebook{width:260px; float:left; margin:20px auto}
.loginFacebook p{font-size:14px;color:#000;font-family: 'OpenSansCondensedLight';margin-bottom:15px;font-weight:normal}
.loginFacebook a{color:#13345c;font-family: 'OpenSansCondensedBold';}
.facebookFriends{margin-bottom:15px;}
.facebookFriends li{float:left;margin-right:3px}
.facebookFriends li img{}
.loginFacebook a.btnFaceLogin{background:url(../images/LoginAndRegister/facebookBtn-bg.png) no-repeat;display:block; width:208px; height:30px;text-align:center;padding:10px 0 0 15px;color:#fff;font-family:Arial, Helvetica, sans-serif}
.btnFaceLogin:hover{color:#fff}
.loganManual{width:340px; float:left}
form td.name{width:100px;text-align:right}
form label.name{font-family: 'OpenSansCondensedLight';font-size:16px;color:#333;font-weight:bold;margin-right:15px}
form input{width:217px;-webkit-border-radius:5px;-moz-border-radius:5px;border-radius:5px;border:1px solid #999;color:#333; padding:5px 10px;font-size:12px;font-family: 'OpenSansLight';margin-bottom:5px;}
form input[type="email"]{outline:none;box-shadow:none;-moz-box-shadow:none;-webkit-box-shadow:none;}
form input[type="submit"]{background:url(../images/LoginAndRegister/btnSign-bg.png) repeat-x; text-align:center;font-size:16px;font-weight:bold;color:#fff;width:78px;-webkit-border-radius:5px;-moz-border-radius:5px;border-radius: 5px;margin-top:-3px;height:32px;font-family: 'OpenSansCondensedBold';font-weight:normal;cursor:pointer;border:none}
form input[type="file"]{margin-top:15px;margin-left:-50px}
form a{font-family: 'OpenSansCondensedLight';font-size:14px; color:#13345c;font-weight:normal}
form input[type="submit"]:hover{background:url(../images/LoginAndRegister/btnSign-bg-hover.png) repeat-x;}
form a.btnCancel{font-size:12px;margin:5px 15px;font-weight:normal;font-family: 'OpenSansRegular';}
.userPhoto{margin-left:50px;float:left;width:150px;margin-top:15px}
.userPhoto img{margin-top:15px}
.formMessage{color:#ff0000;font-family: 'OpenSansCondensedBold';font-size:13px;margin:10px}
.photoContainer{position:relative}
.photoContainer a.btnChangePhoto{position:absolute; bottom:5px; left:90px; display:block; background-color:#0b68a8; width:60px; color:#fff; text-align:center;font-size:10px;padding:6px 0;font-family: 'OpenSansRegular';}
div#forgotPassword a{font-family: 'OpenSansCondensedBold';font-size:14px; color:#13345c;font-weight:normal;position:relative;left:8px;top:-7px}

/* Shadow Effect
--------------------------------------------------------------------------------------- */
.shadowEffect{background:url(../images/LoginAndRegister/hRuler-bg.jpg) no-repeat center left;height:26px;margin-bottom:10px}

/* SideBar
--------------------------------------------------------------------------------------- */
.sidebar{width:210px;float:left;padding-bottom:30px}
.sidebar a{float:right;font-size:18px;color:#13345c;font-family: 'BebasNeueRegular';font-weight:normal;margin-top:20px}
.sidebar h1{font-size:26px;color:#0b68a8;font-family: 'BebasNeueRegular';font-weight:normal;text-align:center;margin-bottom:15px	}
.sidebarBlock{width:180px;background:url(../images/LoginAndRegister/sidebarBlock-bg.jpg) no-repeat center bottom; padding:8px;overflow:visible}
.sidebarBlock img{float:left;margin-right:5px}
.text{width:150px;float:left}
.sidebarBlock p{font-size:12px;color:#13345c}
.sidebarBlock a{float:right;font-size:8px;color:#13345c;margin:10px 0 20px;font-family: 'OpenSansLight';}

/* Article 
--------------------------------------------------------------------------------------- */
.MainContent{width:952px;margin-left:50px;float:right}
.MainNav{margin-bottom:10px}
.MainNav nav{list-style:none}
.MainNav nav li{float:left;padding:5px 20px;height:20px;text-align:center;}
.MainNav nav li a{color:#fff;font-family: 'BebasNeueRegular';font-size:18px;font-weight:normal;text-shadow: 0px -1px 1px #3e89ad;filter: dropshadow(color=#000, offx=0, offy=-1);}
.MainNav nav li.selected{background-color:#0b68a8}
.MainNav nav li.last{margin-right:0}
.MainNav nav li:hover{background-color:#0b68a8}
.MainNav nav li:hover a{}
.MainNav .navlinksLvL1{background-color:#13345c;list-style:none}
.MainNav .navlinksLvL1 li{padding:5px 20px;}
.MainNav .navlinksLvL2{background-color:#50b5e5}
.MainNav .navlinksLvL2 li{padding:5px 16px;}
.MainContentmap{}
.MainContentmap img{}

/* Login & Registration : Email in use popup 
--------------------------------------------------------------------------------------- */
.emailInUse{background:url(../images/LoginAndRegister/emailiuse-img.png) no-repeat;width:242px;height:67px;padding:25px 25px 25px;position:absolute;right:460px;top:-15px}
.emailInUse p{font-family: 'OpenSansCondensedLight';font-size:16px;color:#333;font-weight:normal}

/* Login & Registration : Wrong credentials popup 
--------------------------------------------------------------------------------------- */
.wrongCredentials{background:url(../images/LoginAndRegister/emailiuse-img.png) no-repeat;width:242px;height:97px;padding:15px 20px 20px;position:absolute;right:0;top:-35px}
.wrongCredentials p{font-family: 'OpenSansCondensedLight';font-size:16px;color:#333;font-weight:normal;position:relative;top:5px}

/* Login & Registration : Divider 
--------------------------------------------------------------------------------------- */
#LoginDividerContainer{width:390px}
.loginDividerBox{position:relative;border:1px solid #000;padding:5px;}
.loginDividerBox h2{font-family: 'OpenSansCondensedLight';font-size:24px;color:#000;width:80%}
.loginDividerBox a.loginDividerEditBtn{position:absolute;top:0;right:0;display:none}
.loginDividerBox:hover a.loginDividerEditBtn{display:block}

.loginDividerBox ul.loginDividerFromList{float:right;margin:10px 0}
.loginDividerBox ul.loginDividerFromList li{float:left;margin-right:25px}
.loginDividerBox ul.loginDividerFromList li.last{margin-right:0;}
.loginDividerBox ul.loginDividerFromList li a{}
.loginDividerBox form{margin-top:10px}
.loginDividerBox .tbl_LoginDivider{width:100%}
.loginDividerBox .tbl_LoginDivider label{font-family:"Myriad Pro", Arial, sans-serif;font-size:13px;font-weight:normal;color:#000}
.loginDividerBox .tbl_LoginDivider select.LoginEditSelect{margin-bottom:5px;width:95%;border:1px solid #ccc;-webkit-border-radius:5px;-moz-border-radius:5px;border-radius:5px;font-size:12px;color:#13345c;padding:3px 5px}
.loginDividerBox .tbl_LoginDivider td.LoginEditFormName{width:20%;margin-right:5px}
.loginDividerBox .tbl_LoginDivider td.LoginEditFormInput{width:80%}

/* Login & Registration : LandingPage 
--------------------------------------------------------------------------------------- */
.LoginLandPage{padding:40px 30px}
.LoginLandPageText{position:relative}
.LoginLogo{width:30%;float:left}
.LoginText{width:65%;text-align:center;float:left;margin-top:65px}
.LoginText h1{font-family:Arial, Helvetica, sans-serif;font-weight:normal;text-transform:none;line-height:1.2em;margin-bottom:25px;color:#0b68a8}
.LoginText h2{line-height:1.6em;}
.LoginText p{font-family: 'OpenSansCondensedLight';line-height:1.6em;}
.LoginText p span{font-size:20px;font-family: 'OpenSansCondensedLight';font-size:20px}
.LoginText span{font-size:20px;font-family: 'OpenSansCondensedLight';text-transform:none}
.LoginText a{text-align:center;}
.LoginText a h1{font-size:28px;margin-bottom:0}
.LoginText a img{margin-top:20px}
.LoginText p a.signin{font-family:Arial, Helvetica, sans-serif;font-weight:Bold;text-transform:none;font-size:25px}
.beta{background:url(../images/landingPage/beta.png) no-repeat;width:222px;height:167px;display:block;position:absolute;top:140px;left:75px;text-indent:-9999px}
.LoginLandPageMain{margin-top:100px}
.LoginLandPageMain img{}
.LoginLandPageMain a.LoginAppStoreBtn{float:right; margin:20px 10px 0}


