/* Global
--------- */
.col{float:left}
.colRight{float:right}
.lastcol{float:none;display:block;display:table-cell;width:9999em;*width:auto;*zoom:1;}
.block:before,.block:after{content:"";display:table;}
.block:after{clear:both;}

/* Header
---------------------------------------------------------------------- */
header {
	background: none;
	height: 42px;
	padding:10px 0;
	position: relative;
}
.wrapper{width:1280px; margin:0 auto}
.header{height: 42px\9;padding:10px 0\9;
	position: relative\9;}
.header_shadow {
	background: url(../images/myPage/backgrounds/header-shadow-ng.png) repeat-x;
	height: 34px;
	position:absolute;
	z-index:1;
	width: 100%
}
.logo {
	width: 200px;
	float: left
}
.logo a {
	background: url(../images/general/tn-logo.png) no-repeat;
	width: 179px;
	height: 41px;
	display: block;
	text-indent: -999px
}
.navigation-icons {
	float: right;
	margin-top: 10px
}
.navigation-icons ul {
	width: 220px
}
.navigation-icons ul li {
	float: left;
	margin-right: 15px
}
.navigation-icons ul li.last {
	margin-right: 0;
}
.navigation-icons ul li a {
}
.userAuthentication {
	float: right;
	margin-right: 10px;
	margin-top: 10px
}
.userAuthentication ul {
}
.userAuthentication ul li {
	float: left;
	margin-right: 10px
}
.userAuthentication ul li a {
	color: #13345c;
}

/* Edit Content
---------------------------------------------------------------------- */

.content {
	background: #73c2e7 url(../images/general/bgrd.jpg) repeat;
	padding: 15px 0 50px;
}
.edit {
	box-shadow: 0 0 5px #888888;
	width: 716px;
	margin: 0 auto;
	border: 1px solid #ccc;
	background-color: #fff;
	padding: 0 50px


}
}
.edit h1 {
	text-transform: uppercase;
	color: #06315d;
	font-family: 'OpenSansCondensedBold';
}
.hRuler {
	background: url(../images/general/hRuler-ng.png) -70px 0px no-repeat;
	width: 853px;
	height: 1px;
	display: block;
}
label {
	font-size: 18px;
	color: #333
}
label.secrecy {
	font-size: 14px
}
input[type="text"], input[type="email"] {
	width: 192px;
	height: 25px;
	line-height: 1.6em;
-webkit-border-radius: 5px-moz-border-radius: 5px;
	border-radius: 5px;
	border: 1px solid #ccc;
	padding: 0 10px;
	margin: 0 5px 0 50px
}
.formBlock {
	width: 610px;
	margin: 15px auto;
	font-family: 'OpenSansCondensedLight';
	position: relative
}
.formName {
	width: 80px;
	text-align: right;
	float: left;
}
.form_btn {
	background: url(../images/general/btnChangePassword-bg.png) repeat-x;
	height: 33px;
	text-align: center;
	border: 1px solid #2484be;
	color: #fff;
	-webkit-border-radius: 5px;
	-moz-border-radius: 5px;
	border-radius: 5px;
	margin-left: 50px;
	font-weight: bold
}
.profileImage {
	float: left;
	margin-bottom: 15px;
	cursor: pointer;
	margin-left: 50px
}
.btnOn {
	float: left;
	display: block;
	margin-left: 50px;
	margin-right: 20px;
	cursor: pointer;
}
.btnOff {
	float: left;
	display: block;
	margin-left: 130px;
	margin-right: 20px;
	cursor: pointer;
}
.btnLearnmore {
	position: absolute;
	right: 0;
	top: 60px;
	color: #06315d;
	font-size: 14px;
	font-weight: bold
}
.form_btn_save {
	background: url(../images/general/btnSave-bg.png) repeat-x;
	height: 49px;
	border: 1px solid #ccc;
	text-align: center;
	color: #ccc;
	font-size: 24px;
	font-weight: bold;
	-webkit-border-radius: 5px;
	-moz-border-radius: 5px;
	border-radius: 5px;
	margin-left: 130px;
	margin-bottom: 30px;
	padding: 0 20px
}

#cancelButton{
  margin-left:40px;
  text-decoration:none;
  color:#13345c;
  font-weight:bold;
  font-size:18px;
}

#cancelButton:hover{
 color:#2484BE 
}

/* myPage "Inner Page"
---------------------------------------------------------------------- */
.myPage{}
.myPageHor_ruler{background:url(../images/myPage/backgrounds/myPageHor_ruler-bg.png) ; width:685px;height:22px;border:none;}

/* User Profile Section
-----------------------*/
.userProfile{width:246px;padding:44px 25px 0 20px; background:#f7f7f7; font-family: 'OpenSansCondensedBold';min-height:1132px}
.userInfo{position:relative; margin-bottom:10px}
.userInfo img{margin:0 15px 10px 0}
.userInfo h1{margin:20px 0 40px 0; font-size:24px; color:#333;width:75px;font-family: 'OpenSansCondensedBold';}
.userInfo a.user_editPhoto{position:absolute; top:125px; left:90px; display:block; background-color:#0b68a8; width:60px; color:#fff; text-align:center;font-size:10px;padding:6px 0;}

.userInfo a.btn_addFriend{display:block; background:url(../images/myPage/backgrounds/btn_bg.jpg) repeat-x; width:123px;height:11px ;text-align:center;text-transform:capitalize;font-size:16px;color:#13345c; border:1px solid #ccc; padding:10px 0;-webkit-border-radius: 5px;-moz-border-radius: 5px;border-radius: 5px; margin-right:5px}
.userInfo a.btn_follow{display:block; background:url(../images/myPage/backgrounds/btn_bg.jpg) repeat-x; width:108px;height:11px ;text-align:center;text-transform:capitalize;font-size:16px;color:#13345c; border:1px solid #ccc; padding:10px 0;-webkit-border-radius: 5px;-moz-border-radius: 5px;border-radius: 5px;}

.contactUser{margin-bottom:10px}
.contactUser form{width:245px;}
.contactUser label{font-size:18px; color:#333;}
.contactUser textarea{margin:15px 0; width:243px; height:55px;-webkit-border-radius: 5px;-moz-border-radius: 5px;border-radius: 5px;border:1px solid #979797; }
.contactUser input[type="submit"].btn_sendText{display:block; background:url(../images/myPage/backgrounds/btn_bg.jpg) repeat-x; width:74px; height:31px;font-family: 'OpenSansCondensedBold'; font-size:16px;  text-align:center; text-transform:capitalize; color:#13345c; border:1px solid #ccc;-webkit-border-radius: 5px;-moz-border-radius: 5px;border-radius: 5px; cursor:pointer}

.userMsg{font-family: 'OpenSansCondensedBold';}
.userMsg h2{margin:15px 0; font-size:18px;color:#333;}
.userMsg-block{margin:25px 0; font-family: 'OpenSansCondensedLight';}
.userMsg-block p{font-size:16px; color:#000;}
.userMsg-block span.bubble_top{ background:url(../images/myPage/backgrounds/message/top-message-bgrd.png) no-repeat top; display:block; padding:19px 18px 0; width:209px}
.userMsg-block span.bubble_mid{background:url(../images/myPage/backgrounds/message/middle-message-bgrd.png) repeat bottom; display:block; padding:0 18px;width:209px;font-family: 'OpenSansCondensedLight';}
.userMsg-block span.bubble_bot{background:url(../images/myPage/backgrounds/message/bottom-message-bgrd.png) no-repeat bottom; display:block; padding:33px 18px 0;width:209px}
.userMsg-block span.bubble_bot_left{background:url(../images/myPage/backgrounds/message/bottom-message-bgrd-left.png) no-repeat bottom; display:block; padding:38px 18px 0;width:209px}
.userMsg-Name{font-family: 'OpenSansCondensedBold';}
.userMsg-Name h3{margin:0 20px; font-size:12px;}





/* Main Content Section
-----------------------*/

.mainContent{width:698px; padding-top:50px;font-family: 'OpenSansCondensedBold'; border-right:1px solid #ccc; border-left:1px solid #ccc; padding-bottom:8px;padding-bottom:5px\9}
.mainContent h1{font-size:26px; color:#13345c; margin:0 15px 20px}
.mainContent_block{padding:0 19px;}
.mainContent_block h2{font-size:24px; color:#0b68a8; margin:20px 0;}
.mainContent_block a.btn_more{display:block;background:url(../images/myPage/backgrounds/view-all-icon.png) no-repeat;width:44px;height:16px}
.mainContent_block a.btn_more:hover{background:url(../images/myPage/backgrounds/view-all-iconHover.png) no-repeat;}
.mainContent_block ul{ margin-top:10px}
.mainContent_block ul li{margin-right:25px}
.mainContent_block ul li.last{margin-right:0}
.mainContent_block ul li p{font-size:12px; color:#000;margin:10px 0 20px;text-align:center;}
.mainContent_block ul li h3{font-size:21px; color:#0b68a8;margin:10px 0 20px; text-align:center; text-transform:uppercase; width:140px}









/* User Activities Section
-------------------------*/
.userActivities{width:256px;padding:48px 17px 0 16px;font-family: 'OpenSansCondensedBold';background:#f7f7f7;min-height:1128px}
.userActivities a.btn_workSpace{display:block; cursor:pointer; background:url(../images/myPage/backgrounds/btn_workspace-bg.jpg) repeat-x; height:15px; text-align:center; color:#fff; padding:8px 0; border:1px solid #13345c;-webkit-border-radius: 5px;-moz-border-radius: 5px;border-radius: 5px; font-size:16px; margin-bottom:5px}
.userActivities h2{ margin:15px 0; font-size:18px; color:#333}

.userActivities_block{margin-bottom:20px}
.userActivities_block h3{font-family: 'OpenSansCondensedLight'; font-size:16px; color:#333; margin-bottom:10px; text-transform:none}
.userActivities_block a.userActivities_relatedFriends{font-size:14px;color:#0b68a8; float:right}
.userActivities_block ul{}
.userActivities_block ul li{ float:left;margin:0 3px 5px 0;width:48px; height:48px}
.userActivities_block ul li.last{margin-right:0}
.userActivities_block ul li img{}
.userActivities_block ul li img:hover{border:1px solid #0066cc;}


