.nav {
	height:auto;
	float : left;
	margin : 0;
	padding : 0;
}
.nav ul {
	display : block;
	list-style : none;
	margin : 0;
	padding : 0;
}
.nav ul li {
	float : left;
	display : inline;
	margin:0 5px 0 0;
}
.nav ul li a {
	font-family : Arial, Helvetica, sans-serif;
	font-size : 11px;
	color : #666666;
	font-weight : normal;
	display : block;
	float : left;	
	line-height : 15px;	
	padding : 0 5px 0 0 ;
	text-decoration : none;
}
.nav ul li a:hover {
	color : #666666;
	text-decoration:underline;	
}
.nav-ul {
	font-family : Arial, Helvetica, sans-serif;
	font-size : 11px;
	color : #666666;
	font-weight : normal;
	line-height : 15px;
}
